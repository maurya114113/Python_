   "source": [
    "def factorial(n):\n",
    "    product=1\n",
    "    for i in range(1,n+1):\n",
    "        product=product*i\n",
    "    return product\n",
    "n=int(input('Enter a positive number:'))\n",
    "if n<0:\n",
    "    print('Factorial is not defined on negative number')\n",
    "else:\n",
    "    f=factorial(n)\n",
    "    print('Factorial of ' ,n, 'is' , f)\n",
    "    \n"
   ]
  }
